-
  const instockData = [
    {
      name: "30 м²",
      href: PAGES["30m"]["pageHref"],
      price: "1 410 000 ₽",
      imageName: "in-stock-30m",
      alt: "фото дома 30 м²",
    },
    {
      name: "70",
      href: PAGES["70m"]["pageHref"],
      specialSpan: "м²",
      price: "3 440 000 ₽",
      alt: "фото дома 70 м²",
      imageName: "in-stock-70m",
    },
    {
      name: "Баня",
      href: PAGES["bath"]["pageHref"],
      price: "1 150 000 ₽",
      imageName: "in-stock-bath",
      alt: "фото бани",
    },
  ]

section.instock
  .wrapper.animated-appearance
    h2.instock__title У нас есть дома в наличии
    //- p= (JSON.stringify(PAGES["bath"]["pageHref"]))
    ul.instock__list
      each item in instockData
        li.instock__item
          figure.instock__item-figure.image-scalecontainer
            .image-scalebox
              +pictureJpgWebp(item.imageName, 500, 343, item.alt, "instock__item-image", "lazy")
            figcaption.instock__item-caption
              if (item.specialSpan)
                h3.instock__item-name
                  a(href=`${item.href}.html`)
                    span= item.name
                    span= " "
                    span= item.specialSpan
              else
                h3.instock__item-name
                  a(href=`${item.href}.html`)= item.name
              .instock__item-under
                .instock__item-underline
                p.instock__item-price= item.price

